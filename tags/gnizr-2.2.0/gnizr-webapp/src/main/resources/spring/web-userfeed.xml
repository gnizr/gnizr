<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
	"http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="listSubscriptions" class="com.gnizr.core.web.action.feed.ListSubscriptions" singleton="false">
		<property name="userManager">
			<ref bean="userManager"/>
		</property>
		<property name="feedSubscriptionManager">
			<ref bean="feedSubscriptionManager"/>
		</property>
	</bean>

	<bean id="editSubscription" class="com.gnizr.core.web.action.feed.EditSubscription" singleton="false">
		<property name="userManager">
			<ref bean="userManager"/>
		</property>
		<property name="feedSubscriptionManager">
			<ref bean="feedSubscriptionManager"/>
		</property>
	</bean>
</beans>