<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
	"http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="bookmarkPager" class="com.gnizr.core.bookmark.BookmarkPager"
		singleton="true">
		<constructor-arg>
			<ref bean="gnizrDao"/>
		</constructor-arg>
	</bean>
	
	<bean id="forUserPager" class="com.gnizr.core.pagers.ForUserPager"
		singleton="true">
		<constructor-arg>
			<ref bean="gnizrDao"/>
		</constructor-arg>
	</bean>

	<bean id="tagPager" class="com.gnizr.core.pagers.TagPager" singleton="true">
		<constructor-arg>
			<ref bean="gnizrDao"/>
		</constructor-arg>
	</bean>
</beans>